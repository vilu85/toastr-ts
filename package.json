	"module": "./dist/toastr.js",
	"browser": "./dist/toastr.umd.js",
	"types": "./types",
	"exports": {
		".": {
			"import": "./dist/toastr.js",
			"require": "./dist/toastr.umd.js",
			"default": "./dist/toastr.js"
		}
